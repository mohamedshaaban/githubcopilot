@model IEnumerable<Teacher>

<h1>Teacher List</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Id</td>
                <td>@teacher.FirstName</td>
                <td>@teacher.LastName</td>
            </tr>
        }
    </tbody>
</table>
